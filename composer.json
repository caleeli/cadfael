{
    "name": "cadfael/cadfael",
    "description": "Tool for performing static analysis on databases.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Thomas Shone",
            "email": "xsist10@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.10",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.1",
        "phpstan/phpstan": "^0.12.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Cadfael\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cadfael\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/cadfael"
    ]
}
